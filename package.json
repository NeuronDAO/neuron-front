{
  "name": "@neurondao/front",
  "author": "Neuron DAO",
  "version": "0.2.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeuronDAO/neuron-front"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "private": false,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "fontsource-roboto": "^3.0.3",
    "history": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-swipeable-views": "^0.13.9",
    "sanitize.css": "^12.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/node-logger": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "@svgr/rollup": "^5.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-is": "^16.13.1",
    "rollup": "^2.28.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-url": "^3.0.1"
  },
  "scripts": {
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint --config ./.eslintrc --fix src/",
    "format": "prettier --config ./.prettierrc --write src/",
    "build": "rollup -c && babel ./src -d ./dist/es",
    "predeploy": "cd example && yarn install && yarn run build",
    "rollup:watch": "rollup -c -w",
    "babel:watch": "babel ./src -d ./dist/es --watch",
    "start:example": "react-scripts start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "dist"
  ]
}
